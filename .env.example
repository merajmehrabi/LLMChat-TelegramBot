# Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
OPENROUTER_API_KEY=your_openrouter_api_key
NODE_ENV=development

# Database
MONGODB_URI=mongodb://localhost:27017/llm-chat-bot

# Admin Configuration
